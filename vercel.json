{
    "version": 2,
    "framework": null,
    "buildCommand": null,
    "outputDirectory": null,
    "installCommand": "npm install",
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/$1"
        },
        {
            "src": "/(.*)",
            "dest": "/api/index"
        }
    ],
    "functions": {
        "api/index.ts": {
            "runtime": "edge"
        }
    }
}